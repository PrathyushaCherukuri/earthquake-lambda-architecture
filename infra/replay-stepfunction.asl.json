{
  "Comment": "Replay failed earthquake events from DLQ back to Kinesis",
  "StartAt": "ReplayFailedEvents",
  "States": {
    "ReplayFailedEvents": {
      "Type": "Task",
      "Resource": "arn:aws:states:::lambda:invoke",
      "Parameters": {
        "FunctionName": "earthquake-replay"
      },
      "End": true
    }
  }
}
